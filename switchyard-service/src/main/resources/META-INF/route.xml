<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://camel.apache.org/schema/spring">
    <route>
        <from uri="switchyard://CsvProcessor"/>
        <split>
            <tokenize token="\n"></tokenize>
            <process ref="CsvRecordProcessor"/>
            <to uri="switchyard://TicketEnricher"/>
        </split>
    </route>
</routes>
